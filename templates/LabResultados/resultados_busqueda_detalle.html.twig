<h4 class="text-primary"> <strong><i class="fa fa-check" style="color:lightseagreen;"></i> Orden N&deg; {{ idOrden }}</strong></h4>
<div class="table-responsive">
    <table id="listarExamenesOrden" class="table table-striped table-condensed">
        <thead class="bg-success">
            <tr>
                <th class="text-center">Examen</th>
                <th class="text-center">Identificador</th>
                <th class="text-center">Estado</th>
                <th style="padding-left: 40px;">Accion</th>
            </tr>
        </thead>
        <tbody>
            {% for dato in datos %}
            <tr>
                <td style="width: 25%; text-align:center;"><div class="btn btn-success btn-block" id="idExamen" onclick="mostrarElementos({{ dato.id_examen }}, {{dato.id}},{{idOrden}},{{dato.id_estado_orden}})">{{ dato.nombre_examen }}</div></td>
                <td style="width: 20%; text-align:center;">{{ dato.id }}</td>
                <td style="display: none;">{{ dato.id_examen }}</td>
                <td style="width: 20%; text-align:center;">{{ dato.estado_examen }}</td>
                <td style="width: 35%; text-align:left;">
                    
                    {% if dato.estado_examen == 'Digitado' %}
                    <div class="btn btn-warning" id="" disabled="disabled">Borrar Examen</div>
                    {% endif %}
                    {% if dato.estado_examen == 'Completo' %}
                    <div class="btn btn-danger" id="btnBorrarResEx">Borrar Resultado</div>
                        {% if dato.id_examen == 6%}
                        <a id="btnImprimirExamenHeces" href="#" class="btn btn-primary"><i class="glyphicon glyphicon-print"></i></a>
                        {% elseif dato.id_examen == 16%}
                        <a id="btnImprimirExamenOrina" href="#" class="btn btn-primary"><i class="glyphicon glyphicon-print"></i></a>
                        {% elseif dato.id_examen == 4%}
                        <a id="btnImprimirHemograma" href="#" class="btn btn-primary"><i class="glyphicon glyphicon-print"></i></a>
                        {% else %}
                        <a id="btnImprimirOrden" href="#" class="btn btn-primary"><i class="glyphicon glyphicon-print"></i></a>
                        {% endif %}
                    {% endif %}
                    
                        
                    
                    {# <a id="btnImprimirEx" href="#" class="btn btn-warning"><i class="glyphicon glyphicon-print"></i></a> #}
                    <!-- href="" target="_blank" -->
                </td>
                <td style="display: none;">{{ idOrden }}</td>
            </tr>
            {% endfor %}
            {# <tr>
                <td style="display: none;">{{ idOrden }}</td>
                {% if datos[0].id_estado_orden == '2' %}
                <td colspan="4" class="text-left">
                    <a id="btnImprimirOrden" href="#" class="btn btn-primary">
                        Imprimir Orden
                        <i class="glyphicon glyphicon-print"></i>
                    </a>
                </td>
                {% endif %}
            </tr> #}
        </tbody>
    </table>
</div>