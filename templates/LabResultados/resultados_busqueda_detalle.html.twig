<div class="table-responsive">
    <table id="listarExamenesOrden" class="table table-striped table-condensed">
        <thead class="bg-success">
            <tr>
                <th class="text-center">Examen</th>
                <th class="text-center">Identificador</th>
                <th class="text-center">Estado</th>
                <th class="text-center">Accion</th>
            </tr>
        </thead>
        <tbody class="text-center">
            {% for dato in datos %}
            <tr>
                <td style="width: 25%;"><div class="btn btn-success btn-block" id="idExamen" onclick="mostrarElementos({{ dato.id_examen }}, {{dato.id}},{{idOrden}})">{{ dato.nombre_examen }}</div></td>
                <td style="width: 25%;">{{ dato.id }}</td>
                <td style="width: 25%;">{{ dato.estado_examen }}</td>
                <td style="width: 25%;"><div class="btn btn-danger" id="btnBorrarEx">Borrar Examen</div></td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>