2<div class="table-responsive">
    <table id="listarExamenesOrden" class="table table-striped table-condensed">
        <thead class="bg-success">
            <tr>
                <th class="text-center">Examen</th>
                <th class="text-center">Identificador</th>
                <th class="text-center">Estado</th>
                <th class="text-center">Accion</th>
            </tr>
        </thead>
        <tbody class="text-center">
            {% for dato in datos %}
            <tr>
                <td style="width: 25%;"><div class="btn btn-success btn-block" id="idExamen" onclick="mostrarElementos({{ dato.id_examen }}, {{dato.id}},{{idOrden}})">{{ dato.nombre_examen }}</div></td>
                <td style="width: 20%;">{{ dato.id }}</td>
                <td style="display: none;">{{ dato.id_examen }}</td>
                <td style="width: 20%;">{{ dato.estado_examen }}</td>
                <td style="width: 35%;">
                    
                    {% if dato.estado_examen == 'Digitado' %}
                    <div class="btn btn-danger" id="btnBorrarEx">Borrar Examen</div>
                    {% else %}
                    <div class="btn btn-danger" id="btnBorrarResEx">Borrar Resultado</div>
                    {% endif %}
                        
                    
                    <a id="btnImprimirEx" href="#" class="btn btn-warning"><i class="glyphicon glyphicon-print"></i></a>
                    <!-- href="" target="_blank" -->
                </td>
                <td style="display: none;">{{ idOrden }}</td>
            </tr>
            {% endfor %}
            {# <tr>
                <td colspan="4" class="">
                    <a id="btnImprimirOrden" href="#" class="btn btn-warning">
                        Imprimir Orden
                        <i class="glyphicon glyphicon-print"></i>
                    </a>
                </td>
            </tr> #}
        </tbody>
    </table>
</div>