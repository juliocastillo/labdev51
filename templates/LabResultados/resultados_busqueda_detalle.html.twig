<div class="table-responsive">
    <table id="listarOrdenes" class="table table-striped table-condensed">
        <thead class="bg-success">
            <tr>
                <th class="text-center">Examen</th>
                <th class="text-center">Identificador</th>
                <th class="text-center">Estado</th>
                <th class="text-center">Accion</th>
            </tr>
        </thead>
        <tbody class="text-center">
            {% for dato in datos %}
            <tr>
                <td><div class="btn btn-success" id="idExamen" onclick="mostrarElementos({{ dato.id_examen }})">{{ dato.nombre_examen }}</div></td>
                <td>{{ dato.id_examen }}</td>
                <td>{{ dato.estado_examen }}</td>
                <td><div class="btn btn-danger">Borrar Examen</div></td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>